<!--pages/home/childComps/z-recommends/z-recommends.wxml-->
<view class="recommends">
  <block wx:for="{{recommends}}"
       wx:key="item">
    <view class="recommends-item">
      <image src="{{item.image}}" bindload="handleImageLoad"/>
      <view class="item-text">{{item.title}}</view>
    </view>
  </block>
</view>
