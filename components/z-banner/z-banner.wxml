<!--components/banner/banner.wxml-->
<swiper class="swiper"
        circular
        autoplay
        interval="3000"
        indicator-dots
        indicator-active-color="#ff5777">
  <block wx:for="{{list}}" wx:key="item">
    <swiper-item class="swiper-item">
      <image src="{{item.image}}" mode="widthFix"/>
    </swiper-item>
  </block>
</swiper>
